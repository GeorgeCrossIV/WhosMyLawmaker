@model IEnumerable<Term>

<div class="card">
    <div class="card-header">Terms</div>
    <div class="card-body">
        <div class="row">
            @foreach (Term term in Model)
            {
                <partial name="~/Views/Lawmakers/TermView.cshtml" model=term />
            }

        </div>
    </div>
</div>
